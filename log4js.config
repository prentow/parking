    {
        "category": "fileLogger",
        "type": "file",
        "filename": "log.log",
        "maxLogSize": 10240,
        "backups": 3,
        "layout": {
            "type": "pattern",
            "pattern": "%d{dd/MM hh:mm} %-5p %m"
        }
    }
],
"replaceConsole": true }